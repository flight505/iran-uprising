{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "no-referrer"
        },
        {
          "key": "Permissions-Policy",
          "value": "geolocation=(), camera=(), microphone=(), interest-cohort=()"
        },
        {
          "key": "Alt-Svc",
          "value": "h2=\"einjz5dwp2vztvqni5hf6wthy5izdokfw4d2fgr53t5yy3rzdci55zqd.onion:443\"; ma=86400"
        }
      ]
    }
  ]
}
